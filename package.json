  "devDependencies": {
    "@orbs-network/ton-access": "^2.3.3",
    "@release-it/keep-a-changelog": "^3.1.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.13",
    "jest": "^29.5.0",
    "node-fetch": "^3.3.1",
    "release-it": "^15.10.1",
    "ton": "^13.4.1",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "ton-core": ">=0.49.0",
    "ton-crypto": ">=3.2.0"
  },
  "peerDependencies": {
    "ton-core": ">=0.49.0",
    "ton-crypto": ">=3.2.0"
  },
  "publishConfig": {
